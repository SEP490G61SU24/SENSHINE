<script>
    $(document).ready(function () {
        $('.nav-item').click(function () {
            $('.nav-item').removeClass('active');
            $(this).addClass('active');
        });
    });


    document.addEventListener('DOMContentLoaded', function () {
        // Khởi tạo sự kiện click cho tất cả các liên kết tab
        var tabLinks = document.querySelectorAll('.nav-tabs a');
        tabLinks.forEach(function (link) {
            link.addEventListener('click', function (event) {
                event.preventDefault(); // Ngăn chặn hành vi mặc định của liên kết

                // Lấy id của tab pane từ thuộc tính href của liên kết
                var targetId = this.getAttribute('href').substring(1);

                // Hiển thị tab pane tương ứng
                showTab(targetId);
            });
        });

        // Hiển thị tab mặc định khi trang tải
        function showTab(tabId) {
            // Ẩn tất cả các tab pane
            var tabPanes = document.querySelectorAll('.tab-pane');
            tabPanes.forEach(function (pane) {
                pane.classList.remove('show', 'active');
            });

            // Bỏ class active khỏi tất cả các liên kết tab
            var tabs = document.querySelectorAll('.nav-tabs a');
            tabs.forEach(function (tab) {
                tab.classList.remove('active');
            });

            // Hiển thị tab pane tương ứng với id được truyền vào
            var activeTab = document.getElementById(tabId);
            if (activeTab) {
                activeTab.classList.add('show', 'active');
            }

            // Đánh dấu liên kết tab là active
            var activeLink = document.querySelector('.nav-tabs a[href="#' + tabId + '"]');
            if (activeLink) {
                activeLink.classList.add('active');
            }
        }

        // Hiển thị tab mặc định khi trang tải
        showTab('signature-packages'); // Tab mặc định hiển thị khi trang tải
    });
</script>



