<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <div class="sidebar">
        <div class="logo">DashUI</div>
        <nav>
            <ul>
                <li>Dashboard</li>
                <li>Projects</li>
                <li>Layouts & Pages</li>
                <li>UI Components</li>
                <li>Documentation</li>
            </ul>
        </nav>
    </div>
    <div class="main-content">
        <header>
            <input type="search" placeholder="Search">
            <div class="user-profile">
                <img src="profile.jpg" alt="User Profile">
            </div>
        </header>
        <section class="stats">
            <div class="stat-card">
                <h3>Projects</h3>
                <p>18</p>
                <small>2 Completed</small>
            </div>
            <div class="stat-card">
                <h3>Active Task</h3>
                <p>132</p>
                <small>28 Completed</small>
            </div>
            <div class="stat-card">
                <h3>Teams</h3>
                <p>12</p>
                <small>1 Completed</small>
            </div>
            <div class="stat-card">
                <h3>Productivity</h3>
                <p>76%</p>
                <small>5% Completed</small>
            </div>
        </section>
        <section class="projects">
            <h2>Active Projects</h2>
            <table>
                <thead>
                    <tr>
                        <th>Project name</th>
                        <th>Hours</th>
                        <th>Priority</th>
                        <th>Members</th>
                        <th>Progress</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>File Management App</td>
                        <td>34</td>
                        <td>Medium</td>
                        <td><img src="members.jpg" alt="Members"></td>
                        <td>15%</td>
                    </tr>
                    <tr>
                        <td>Slack Team UI Design</td>
                        <td>34</td>
                        <td>Medium</td>
                        <td><img src="members.jpg" alt="Members"></td>
                        <td>25%</td>
                    </tr>
                    <tr>
                        <td>GitHub Satellite</td>
                        <td>34</td>
                        <td>Medium</td>
                        <td><img src="members.jpg" alt="Members"></td>
                        <td>15%</td>
                    </tr>
                    <tr>
                        <td>3D Character Modelling</td>
                        <td>34</td>
                        <td>High</td>
                        <td><img src="members.jpg" alt="Members"></td>
                        <td>15%</td>
                    </tr>
                    <tr>
                        <td>Webapp Design System</td>
                        <td>34</td>
                        <td>High</td>
                        <td><img src="members.jpg" alt="Members"></td>
                        <td>15%</td>
                    </tr>
                </tbody>
            </table>
        </section>
        <section class="tasks-performance">
            <h2>Tasks Performance</h2>
            <div class="performance-chart">
                <!-- Placeholder for performance chart -->
            </div>
        </section>
        <section class="teams">
            <h2>Teams</h2>
            <table>
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Role</th>
                        <th>Last Activity</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Jitu Chauhan</td>
                        <td>Front End Developer</td>
                        <td>Today</td>
                    </tr>
                    <tr>
                        <td>Sandeep Chauhan</td>
                        <td>Project Director</td>
                        <td>Today</td>
                    </tr>
                </tbody>
            </table>
        </section>
    </div>
</body>
</html>
